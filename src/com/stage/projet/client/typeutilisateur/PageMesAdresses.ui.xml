<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">
	
	<g:HTMLPanel>
		<g:Grid width="694px" height="36px">
			<g:row>
				<g:customCell>
					<g:Image url="Entete12.png"/>
				</g:customCell>
			</g:row>
			
			
			
			
			<g:row>
				<g:customCell>
					<g:HorizontalPanel>
						<g:HTML wordWrap="true" width="850px" height="2px"/>
						<g:Label width="250px" ui:field="MonNom" height="4px"/>
						<g:HTML wordWrap="true" width="100px"></g:HTML>
						<g:Hyperlink text="Se déconnecter" targetHistoryToken="newHistoryToken" width="130px" height="25" ui:field="Deconnexion"/>
					</g:HorizontalPanel>
				</g:customCell>
			</g:row>
			
			
		</g:Grid>
		
		
		
		<g:Grid width="698px" height="402px">
			<g:row>
				<g:customCell>
					<g:HorizontalPanel width="435px">
						<g:VerticalPanel>
								<g:HTML wordWrap="true" height="40px"></g:HTML>
							<g:Button width="170px" height="50px" ui:field="page_acceuil" text="Accueil">Page d'acceuil</g:Button>
							<g:HTML wordWrap="true" height="20px"></g:HTML>
							<g:Button width="170px" height="50px" ui:field="adresse_dispo">Adresses disponibles</g:Button>
							<g:HTML wordWrap="true" height="20px"></g:HTML>
							<g:Cell verticalAlignment="ALIGN_MIDDLE">
								<g:Button width="170px" height="50px" ui:field="ajouter_terminal" text="Mes terminaux">Ajouter terminal</g:Button>
							</g:Cell>
							<g:HTML wordWrap="true" height="20px"></g:HTML>
							<g:Cell verticalAlignment="ALIGN_MIDDLE">
								<g:Button width="170px" height="50px" text="Mes adresses" ui:field="MesAdresses">Demande affectation</g:Button>
							</g:Cell>
							<g:HTML wordWrap="true" height="20px"></g:HTML>
							<g:Button text="Modifer mes infos" width="170px" height="50px" ui:field="ModifierInfos"/>
							<g:HTML wordWrap="true" width="170px" height="219px"></g:HTML>
							</g:VerticalPanel>
						<g:VerticalPanel width="634px" height="416px">
							<g:HTML wordWrap="true" height="30px"></g:HTML>
							<g:HorizontalPanel width="533px" height="90px">
								<g:HTML wordWrap="true" width="30px" height="12px"></g:HTML>
								<g:ScrollPanel width="650px" height="300px">
									<g:FlexTable ui:field="MaFlexTable" width="100%" height="100%"/>
								</g:ScrollPanel>
								
							</g:HorizontalPanel>
							<g:VerticalPanel ui:field="PanneauModifDate">
								<g:HorizontalPanel>
									<g:Label text="Nom Machine"/>
									<g:TextBox ui:field="NomMachine"/>
									<g:HTML wordWrap="true" width="25px"></g:HTML>
									<g:Label text="Adresse IP"/>
									<g:TextBox ui:field="AdresseAff"/>
								</g:HorizontalPanel>
								<g:HorizontalPanel>
									<g:Label text="Date Debut"/>
									<g:TextBox ui:field="DateD"/>
									<g:HTML wordWrap="true"/>
									<g:Label text="Date Fin"/>
									<p1:DateBox ui:field="DateFin"/>
									<g:HTML wordWrap="true" width="62px"/>
									<g:Button text="Modifier Durée Affectation" ui:field="BModif"></g:Button>
								</g:HorizontalPanel>
							</g:VerticalPanel>
						</g:VerticalPanel>
					</g:HorizontalPanel>
				</g:customCell>
			</g:row>
		</g:Grid>
		<g:Grid width="703px">
			<g:row>
				<g:customCell>
					<g:Image url="Footer1.png"/>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder>  